{
  "expo": {
    "name": "ProjetMEL",
    "slug": "ProjetMEL",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/start.png",
    "splash": {
      "image": "./assets/start.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/start.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "luxembourg.jardin",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "favicon": "./assets/start.png"
    }
  }
}